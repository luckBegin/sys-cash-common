{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {
		"no-unused-expression": true
	},
	"rules": {
		"quotemark": [
			true,
			"single"
		],
		"member-access": [
			false
		],
		"ordered-imports": [
			false
		],
		"max-line-length": [
			true,
			150
		],
		"member-ordering": [
			false
		],
		"interface-name": [
			false
		],
		"arrow-parens": false,
		"object-literal-sort-keys": false,
		"no-trailing-whitespace": false ,
		"no-console": false ,
		"indent": true,
		"trailing-comma" : false ,
		"max-classes-per-file" : false,
		"no-bitwise" : false ,
		"class-name" : false ,
		"variable-name": false,
		"no-var-requires": false,
		"no-string-literal": false
	},
	"rulesDirectory": []
}


